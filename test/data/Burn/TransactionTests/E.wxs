<?ifndef Version?>
<?define Version = 1.0.0.0?>
<?endif?>

<Wix xmlns="http://wixtoolset.org/schemas/v4/wxs">
  <Product Id="*" Name="~$(var.TestName) - E" Language="1033" Version="$(var.Version)" Manufacturer="Microsoft Corporation"
           UpgradeCode="{04ABCDBB-2C66-4338-9B1D-DE2AC9B0D1C2}">
    <Package Compressed="yes" InstallerVersion="405" InstallScope="perMachine"/>

    <MajorUpgrade DowngradeErrorMessage="E newer version of [ProductName] is already installed." />
    <MediaTemplate />

    <Property Id="MsiLogging" Value="voicewarmup"/>
    <PropertyRef Id="TestVersion"/>

    <Feature Id="Complete" Level="1">
      <ComponentRef Id="FileComponent"/>
    </Feature>
    
  </Product>

  <Fragment>
    <Directory Id="TARGETDIR" Name="SourceDir">
      <Directory Id="ProgramFilesFolder">
        <Directory Id="WixDir" Name="~Test WiX">
          <Directory Id="TestDir" Name="$(var.TestName)">
            <Directory Id="INSTALLFOLDER" Name="E"/>
          </Directory>
        </Directory>
      </Directory>
    </Directory>
  </Fragment>

  <Fragment>
    <Component Id="FileComponent" Guid="{40D03443-CDDF-45DD-B8EC-2FACA2879324}" Directory="INSTALLFOLDER">
      <File Source="$(sys.SOURCEFILEPATH)"/>
    </Component>
  </Fragment>

  <Fragment>
    <Property Id="TestVersion" Value="$(var.Version)"/>
  </Fragment>
</Wix>
